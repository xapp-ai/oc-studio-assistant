{
  "name": "@xapp/assistant",
  "repository": "github:xapp-ai/oc-studio-assistant",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0",
  "description": "Virtual Assistant for XAPP AI",
  "types": "lib/index",
  "typings": "lib/index",
  "main": "lib/index",
  "files": [
    "lib"
  ],
  "engines": {
    "node": "^12 || ^14"
  },
  "devDependencies": {
    "@types/chai": "4.3.0",
    "@types/cors": "2.8.12",
    "@types/mocha": "9.1.0",
    "@types/node": "14.18.9",
    "@types/sinon": "10.0.2",
    "@types/sinon-chai": "3.2.8",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "@xapp/config": "0.2.3",
    "aws-sdk": "2.1072.0",
    "body-parser": "1.19.1",
    "chai": "4.3.4",
    "eslint": "8.6.0",
    "eslint-plugin-notice": "0.9.10",
    "express": "4.17.2",
    "mocha": "9.1.4",
    "nodemon": "2.0.15",
    "rimraf": "3.0.2",
    "sinon": "12.0.1",
    "sinon-chai": "3.7.0",
    "ts-node": "10.4.0",
    "typescript": "4.5.5"
  },
  "scripts": {
    "clean": "rimraf ./lib/*",
    "build": "tsc -d true -p .",
    "pretest": "rimraf ./lib && tsc",
    "prebuild": "rimraf ./lib",
    "package": "rimraf ./lib && tsc && serverless package",
    "deploy:dev": "rimraf ./lib && tsc && serverless deploy --stage dev",
    "deploy:test": "rimraf ./lib && tsc && serverless deploy --stage test --force",
    "deploy:prod": "rimraf ./lib && tsc && serverless deploy --stage prod",
    "express": "STENTOR_LOG_LEVEL=debug nodemon src/express.ts",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "exit 0",
    "zip": "tsc -d true -p . && node scripts/dist.js"
  },
  "dependencies": {
    "@xapp/assistant-core": "0.0.0",
    "@xapp/question-answering-handler": "2.0.7",
    "@xapp/stentor-lex-connect": "1.40.35",
    "@xapp/stentor-lex-v2": "1.40.35",
    "@xapp/stentor-service-lex": "1.40.35",
    "stentor": "1.52.11",
    "stentor-channel": "1.52.11",
    "stentor-user-storage-dynamo": "1.52.9"
  }
}
